{
	"name": "vitest-create-mock",
	"version": "0.0.3",
	"description": "A utility library for creating mocks in Vitest tests.",
	"type": "module",
	"license": "MIT",
	"homepage": "https://github.com/jakeklassen/vitest-create-mock#readme",
	"bugs": {
		"url": "https://github.com/jakeklassen/vitest-create-mock/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jakeklassen/vitest-create-mock.git"
	},
	"author": "Jake Klassen <jklassendev@gmail.com>",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
