	"packageManager": "pnpm@10.24.0",
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@types/node": "^24.10.1",
		"@vitest/coverage-v8": "^4.0.15",
		"bumpp": "^10.3.2",
		"prettier": "^3.7.4",
		"tsdown": "^0.16.8",
		"typescript": "^5.8.3",
		"vitest": "^4.0.15"
	}
